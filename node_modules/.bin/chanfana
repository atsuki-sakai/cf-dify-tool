#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/chanfana@2.8.1/node_modules/chanfana/dist/node_modules:/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/chanfana@2.8.1/node_modules/chanfana/node_modules:/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/chanfana@2.8.1/node_modules:/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/chanfana@2.8.1/node_modules/chanfana/dist/node_modules:/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/chanfana@2.8.1/node_modules/chanfana/node_modules:/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/chanfana@2.8.1/node_modules:/Users/atsukisakai/Desktop/dify-tool/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/chanfana@2.8.1/node_modules/chanfana/dist/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/chanfana@2.8.1/node_modules/chanfana/dist/cli.js" "$@"
fi

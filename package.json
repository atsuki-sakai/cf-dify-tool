{
  "name": "dify_tool_cloudflare",
  "description": "Dify Tool Cloudflare Worker API to d1 database to store chat history and reservations",
  "cloudflare": {
    "label": "Dify Tool Cloudflare Worker API",
    "products": [
      "Workers",
      "D1"
    ],
    "categories": [
      "storage"
    ],
    "icon_urls": [
      "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/5ca0ca32-e897-4699-d4c1-6b680512f000/public"
    ],
    "docs_url": "https://chanfana.com",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/91076b39-1f5b-46f6-7f14-536a6f183000/public",
    "publish": true
  },
  "scripts": {
    "cf-typegen": "wrangler types",
    "deploy": "wrangler deploy",
    "dev": "pnpm seedLocalDb && wrangler dev",
    "predeploy": "wrangler d1 migrations apply DB --remote",
    "seedLocalDb": "wrangler d1 migrations apply DB --local",
    "schema": "npx chanfana && node scripts/convert-openapi-31-to-30.mjs",
    "test": "wrangler deploy --dry-run && npx vitest run --config tests/vitest.config.mts"
  },
  "dependencies": {
    "chanfana": "2.8.1",
    "drizzle-orm": "^0.44.4",
    "hono": "4.8.2",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.8.44",
    "@types/node": "24.0.4",
    "drizzle-kit": "^0.31.4",
    "typescript": "5.8.3",
    "wrangler": "^4.28.1",
    "vitest": "2.0.5",
    "vite": "5.4.8"
  }
